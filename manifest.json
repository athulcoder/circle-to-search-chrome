{
  "manifest_version": 3,
  "name": "Circle to Search",
  "description": "Select an area on the page and search it with Google Lens",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
